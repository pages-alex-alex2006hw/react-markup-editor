{
  "name": "react-markup-editor",
  "version": "0.0.0",
  "description": "WYSIWYG component for React using draft-js and Markdown",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "example-build": "browserify -t [ babelify --presets [ es2015 react ] ] -t [ stringify --extensions [.md] ] ./example/main.js > ./example/dist.js; lessc ./example/main.less ./example/dist.css",
    "example-deploy": "npm run example-build; gh-pages -d ./example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitBookIO/react-markup-editor.git"
  },
  "keywords": [
    "react",
    "draft-js",
    "markdown",
    "wysiwyg"
  ],
  "author": "Samy Pesse <samypesse@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GitBookIO/react-markup-editor/issues"
  },
  "homepage": "https://github.com/GitBookIO/react-markup-editor#readme",
  "dependencies": {
    "draft-js": "^0.3.0",
    "draft-markup": "^0.1.0",
    "react": "^15.0.0-rc.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "gh-pages": "^0.11.0",
    "less": "^2.6.1",
    "mocha": "^2.4.5",
    "raw-loader": "^0.5.1",
    "react-dom": "^15.0.0-rc.1",
    "reset-css": "^2.0.2011012603",
    "stringify": "^5.1.0",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^4.2.1"
  }
}
